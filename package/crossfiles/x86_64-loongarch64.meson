[binaries]
c = 'loongarch64-unknown-linux-gnu-gcc'
cpp = 'loongarch64-unknown-linux-gnu-c++'
ar = 'loongarch64-unknown-linux-gnu-ar'
strip = 'loongarch64-unknown-linux-gnu-strip'
pkgconfig = 'pkg-config'

[properties]
c_link_args = ['-m32', '-Wl,-z,text']
c_args = ['-m32']

[host_machine]
system = 'loongarch64'
cpu_family = 'x86_64'
cpu = 'x86_64'
endian = 'little'
